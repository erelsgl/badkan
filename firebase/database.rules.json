{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": false,
    ".write": false,
      "userDetails": {
      ".indexOn": "country_id"
    },
    "courses": {
      ".indexOn": "owner_uid"
    },
    "exercises": {
      ".indexOn": "course_id"
    }, 
    "submissions": {
      ".indexOn": ["uid", "exercise_id"] 
    }
  }
}
